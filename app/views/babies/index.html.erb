<h1>heelooooooooo </h1>
<ul>
  <% @babies.each do |baby| %>
    <%= link_to baby.name, baby_path(baby.id) %>
    <li><p>Age</p><%= baby.age %></li>
    <li><p>potty_trained</p><%= baby.potty_trained %></li>
    <li><p>avg_napping_hours</p><%= baby.avg_napping_hours %></li>
    <li><p>screamometer</p><%= baby.screamometer %></li>
    <li><p>medical_needs</p><%= baby.medical_needs %></li>
  <% end %>
</ul>

<div class="container text-center">
  <div class="row">
  <% @babies.each do |baby| %>
    <div class="col"><%= baby.name %></div>
    <div class="col"><%= baby.age %></div>
    <div class="col"><%= baby.potty_trained %></div>
    <div class="col">col</div>
    <% end %>
  </div>


<div style="width: 100%; height: 600px;"
  data-controller="map"
  data-map-markers-value="<%= @markers.to_json %>"
  data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"></div>
